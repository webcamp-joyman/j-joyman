<div class = "col-2">
  <table class = "table table-bordered text-center">
    <thead>
      <tr>
        <th>ジャンル検索</th>
      </tr>
    </thead>
    <tbody>
    <% genres.each do |genre| %>
      <tr>
        <td><%= link_to genre.name,items_genre_search_path(genre), class:"btn btn-success" %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>